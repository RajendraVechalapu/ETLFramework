{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "rajadf2022"
        },
        "Azure_SQLDB_ETLFramework_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'Azure_SQLDB_ETLFramework'"
        },
        "Azure_SQLDB_azadw_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'Azure_SQLDB_azadw'"
        },
        "onprem_SQLDB_AdventureWorks2019_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'onprem_SQLDB_AdventureWorks2019'"
        },
        "SqlServeronpremsourcedb_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'SqlServeronpremsourcedb'"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "rajadf2022_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-02-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "Azure_SQLDB_ETLFramework_connectionString": {
                        "value": "[parameters('Azure_SQLDB_ETLFramework_connectionString')]"
                    },
                    "Azure_SQLDB_azadw_connectionString": {
                        "value": "[parameters('Azure_SQLDB_azadw_connectionString')]"
                    },
                    "onprem_SQLDB_AdventureWorks2019_connectionString": {
                        "value": "[parameters('onprem_SQLDB_AdventureWorks2019_connectionString')]"
                    },
                    "SqlServeronpremsourcedb_connectionString": {
                        "value": "[parameters('SqlServeronpremsourcedb_connectionString')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}